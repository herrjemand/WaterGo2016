[Unit]
Description=uWSGI instance to serve watergo
After=network.target

[Service]
User=developer
Group=www-data
WorkingDirectory=/applications/watergo
Environment="PATH=/applications/watergo/venv/bin"
ExecStart=/applications/watergo/venv/bin/uwsgi --ini uwsgi.ini

[Install]
WantedBy=multi-user.target